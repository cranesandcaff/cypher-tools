{
  "name": "cypher-tools",
  "version": "1.0.0",
  "description": "Various useful tools for Neo4J's Cypher Query Language",
  "main": "build/index.js",
  "scripts": {
    "compile": "babel -d ./build/ ./src/",
    "docs": "jsdoc -c ./.jsdocrc",
    "prepublish": "npm run build && npm run test",
    "build": "npm run docs && npm run compile",
    "pages": "git checkout gh-pages && git merge master && git checkout master",
    "test": "jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:RussellsToolbox/cypher-tools.git"
  },
  "keywords": [
    "neo4j",
    "cypher"
  ],
  "author": {
    "name": "John Mothershed",
    "url": "http://codepen.io/wiseguy12851/"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/RussellsToolbox/cypher-tools/issues"
  },
  "homepage": "https://github.com/RussellsToolbox/cypher-tools#readme",
  "dependencies": {
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.20",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-2": "^6.1.18",
    "eslint": "^1.9.0",
    "jasmine": "^2.3.2",
    "jsdoc": "^3.3.3",
    "node-inspector": "^0.12.3"
  },
  "tonicExampleFilename": "example.js"
}
